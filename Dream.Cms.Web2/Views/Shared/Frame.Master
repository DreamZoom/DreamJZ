    <script type="text/javascript">

        (function ($) {

            $.fn.select = function (option) {
                ///初始化参数
                option = $.extend({
                    Url: $(this).attr("data-Url") || "",
                    Title: $(this).attr("data-Title") || "请选择",
                    Key: $(this).attr("data-Key") || "ID",
                    KeyTitle: $(this).attr("data-KeyTitle") || "编号",
                    Name: $(this).attr("data-Name") || "",
                    NameTitle: $(this).attr("data-NameTitle") || "",
                    Columns: $(this).attr("data-Columns") || "",
                    ColumnNames: $(this).attr("data-ColumnNames") || "",
                    SelectMode: $(this).attr("data-SelectMode") || 1,
                    onLoadSuccess: function (grid) { },
                    onSelect: function () { },
                    onUnSelect: function () { }
                }, option);


                var dialogTemplate = '<div class="modal fade" tabindex="-1" aria-hidden="true"><div class="modal-dialog"><div class="modal-content"><div class="modal-header"><button type="button"class="close"data-dismiss="modal"aria-hidden="true"></button><h4 class="modal-title">请选择</h4></div><div class="modal-body"><div class="row"><div class="col-md-12"><table class="datagrid"style="width:100%;min-height:200px;"></table></div></div></div><div class="modal-footer"><button type="button"data-dismiss="modal"class="btn dark">取消</button><button type="button"class="btn green btn_ok"data-dismiss="modal">确定</button></div></div></div></div>';

                var dialog = $(dialogTemplate).appendTo($("body"));

                dialog.find(".modal-title").html(option.Title || "请选择");

                var btn_ok = dialog.find(".btn_ok");

                $(dialog).on("shown.bs.modal", function () {
                    var datagrid = dialog.find(".datagrid");

                    var fileds = new Array();
                    fileds.push({ field: "ck", title: "全选", checkbox: true, width: 100 });
                    fileds.push({ field: option.Key, title: option.KeyTitle || option.Key, width: 100 });
                    if (option.Key != option.Name) {
                        fileds.push({ field: option.Name, title: option.NameTitle || option.Name, width: 100 });
                    }

                    var columnList = option.Columns.split(",");
                    var columnNameList = option.ColumnNames.split(",");
                    for (var i = 0; i < columnList.length; i++) {
                        fileds.push({ field: columnList[i], title: columnNameList[i] || columnList[i], width: 100 });
                    }
                   
                    $(datagrid).datagrid({
                        url: option.Url,
                        columns: [fileds],
                        pagination:true,
                        singleSelect: (option.SelectMode == 1),
                        width: "100%",
                        onLoadSuccess: function () {
                            option.onLoadSuccess(datagrid);
                        }
                       
                    });

                    $(btn_ok).bind("click", function () {
                        var rows = $(datagrid).datagrid("getChecked");
                        option.onSelect(rows);
                    });
                });
                $(dialog).modal("show");


            }

        })(jQuery);


       

    </script>
